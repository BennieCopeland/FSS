{
  "private": true,
  "type": "module",
  "scripts": {
    "start": "dotnet fable watch ./docs -o ./public --run parcel public/index.html",
    "build": "dotnet fable ./docs -o ./public --run parcel build public/index.html --no-source-maps --public-url .",
    "test": "dotnet fable ./tests -o ./dist/tests --run node ./dist/tests/Tests.js",
    "watch-test": "dotnet fable watch ./tests -o ./dist/tests --run nodemon ./dist/tests/Tests.js",
    "clean": "rm -rf dist && rm -rf public/*.js && rm -rf public/fss",
    "publish-docs": "npm run clean && npm run build && gh-pages -d dist"
  },
  "homepage": "https://Bjorn-Strom.github.io/FSS",
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/plugin-transform-modules-commonjs": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "cssnano": "^4.1.10",
    "nodemon": "^2.0.7"
  },
  "dependencies": {
    "@emotion/css": "11.1.3",
    "gh-pages": "^3.1.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-markdown": "^5.0.3",
    "react-syntax-highlighter": "9.0.1"
  }
}
