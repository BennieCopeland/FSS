{
  "private": true,
  "type": "module",
  "scripts": {
    "start": "dotnet fable watch ./docs -o ./public --run parcel serve public/index.html",
    "build": "dotnet fable ./docs -o ./public --run parcel build public/index.html --public-url .",
    "build-report": "dotnet fable ./docs -o ./public --run parcel build public/index.html --reporter @parcel/reporter-bundle-analyzer --public-url .",
    "test": "dotnet fable ./tests -o ./dist/tests --run node ./dist/tests/Tests.js",
    "watch-test": "dotnet fable watch ./tests -o ./dist/tests --run nodemon ./dist/tests/Tests.js",
    "clean": "rm -rf dist public/*.js public/fss public/FssFeliz public/.fable .parcel-cache",
    "publish-docs": "npm run clean && npm run build && gh-pages -d dist"
  },
  "homepage": "https://Bjorn-Strom.github.io/FSS",
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/plugin-transform-modules-commonjs": "^7.16.0",
    "@babel/preset-react": "^7.16.0",
    "@parcel/reporter-bundle-analyzer": "^2.0.1",
    "cssnano": "^5.0.10",
    "nodemon": "^2.0.15",
    "parcel": "^2.0.1"
  },
  "dependencies": {
    "@emotion/css": "11.1.3",
    "gh-pages": "^3.2.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-markdown": "^5.0.3",
    "react-syntax-highlighter": "^15.4.4"
  }
}
